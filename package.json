{
  "name": "the-better-thing",
  "version": "0.0.0",
  "description": "Discovering the better thing, one claim at a time.",
  "main": "index.js",
  "browserslist": [
    "last 1 Firefox version"
  ],
  "scripts": {
    "clean": "rm -rf build/ node_modules/@def.codes/",
    "format": "prettier --write 'packages/**/*.{ts,json}'",
    "test-format": "prettier --list-different 'packages/**/*.{ts,json}'",
    "build": "npm run build:script && npm run build:style",
    "build:script": "tsc --build",
    "prebuild:style": "bash -c \"mkdir -p build/style\"",
    "build:style": "stylus --out build/style style",
    "bundle": "rollup -c",
    "watch": "bash -c './watch.sh'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/def-codes/the-better-thing"
  },
  "keywords": [
    "dynamic",
    "modeling"
  ],
  "author": "Gavin Cannizzaro <gcannizzaro@mindgrub.com>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@thi.ng/csp": "1.1.6",
    "@thi.ng/dcons": "2.1.5",
    "@thi.ng/defmulti": "1.1.4",
    "@thi.ng/hdom": "9.0.0-alpha1",
    "@thi.ng/hiccup-markdown": "1.1.5",
    "@thi.ng/interceptors": "2.2.1",
    "@thi.ng/rstream": "2.5.5",
    "@thi.ng/rstream-query": "1.1.5",
    "@thi.ng/transducers": "5.4.5",
    "@thi.ng/transducers-fsm": "1.1.5",
    "@thi.ng/transducers-hdom": "2.0.30",
    "d3-force": "2.0.1",
    "d3-require": "1.2.2",
    "requirejs": "2.3.6",
    "tslib": "1.10.0",
    "ws": "7.2.0"
  },
  "devDependencies": {
    "@types/ws": "6.0.3",
    "prettier": "1.19.1",
    "rollup": "1.26.0",
    "stylus": "0.54.5",
    "typescript": "3.7.2"
  },
  "prettier": {
    "trailingComma": "es5",
    "endOfLine": "lf"
  }
}
