{
  "name": "the-better-thing",
  "version": "0.0.0",
  "description": "Discovering the better thing, one claim at a time.",
  "main": "index.js",
  "browserslist": [
    "last 1 Firefox version"
  ],
  "scripts": {
    "clean": "rm -rf build/ node_modules/@def.codes/",
    "format": "prettier --write 'packages/**/*.{ts,json}'",
    "test-format": "prettier --list-different 'packages/**/*.{ts,json}'",
    "build": "npm run build:script && npm run build:style",
    "build:script": "tsc --build",
    "prebuild:style": "bash -c \"mkdir -p build/style\"",
    "xbuild:style": "stylus --out build/style style",
    "build:style": "stylus --out build/style style",
    "bundle": "rollup -c",
    "watch": "bash -c './watch.sh'",
    "start": "node server",
    "port": "live-server --watch=\"packages/amd-loader/test/*.js,*.html,*.css,*.js,build/style,node_modules/@def.codes\" --open=portable.html",
    "meld": "live-server --watch=\"*.html,build/style,node_modules/@def.codes\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/def-codes/the-better-thing"
  },
  "keywords": [
    "dynamic",
    "modeling"
  ],
  "author": "Gavin Cannizzaro <gcannizzaro@mindgrub.com>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@thi.ng/dcons": "2.0.19",
    "@thi.ng/defmulti": "1.0.9",
    "@thi.ng/hdom": "7.2.8",
    "@thi.ng/hiccup-markdown": "1.1.1",
    "@thi.ng/interceptors": "2.0.12",
    "@thi.ng/rstream": "2.4.4",
    "@thi.ng/rstream-query": "1.0.26",
    "@thi.ng/transducers": "5.3.7",
    "@thi.ng/transducers-fsm": "1.1.2",
    "@thi.ng/transducers-hdom": "2.0.23",
    "d3-force": "2.0.1",
    "d3-require": "1.2.2",
    "node-static": "0.7.11",
    "requirejs": "2.3.6",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "live-server": "1.2.1",
    "prettier": "1.18.2",
    "rollup": "1.16.3",
    "stylus": "0.54.5",
    "typescript": "3.5.2"
  },
  "prettier": {
    "trailingComma": "es5"
  }
}
