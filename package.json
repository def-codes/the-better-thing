{
  "name": "the-better-thing",
  "version": "0.0.0",
  "description": "Discovering the better thing, one claim at a time.",
  "main": "index.js",
  "browserslist": [
    "last 1 Firefox version"
  ],
  "scripts": {
    "clean": "rm -rf build/ node_modules/@def.codes/",
    "format": "prettier --write 'packages/**/*.{ts,json}'",
    "meld": "node node_modules/@def.codes/meld-cli",
    "test-format": "prettier --list-different 'packages/**/*.{ts,json}'",
    "build": "tsc --build",
    "bundle": "rollup -c",
    "watch": "bash -c './watch.sh'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/def-codes/the-better-thing"
  },
  "keywords": [
    "dynamic",
    "modeling"
  ],
  "author": "Gavin Cannizzaro <gcannizzaro@mindgrub.com>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@thi.ng/csp": "1.1.22",
    "@thi.ng/dcons": "2.2.15",
    "@thi.ng/defmulti": "1.2.14",
    "@thi.ng/dgraph": "1.2.7",
    "@thi.ng/hdom": "8.0.23",
    "@thi.ng/hiccup-markdown": "1.2.10",
    "@thi.ng/interceptors": "2.2.17",
    "@thi.ng/iterators": "5.1.22",
    "@thi.ng/rstream": "4.0.8",
    "@thi.ng/rstream-query": "1.1.23",
    "@thi.ng/transducers": "6.4.9",
    "@thi.ng/transducers-fsm": "1.1.22",
    "@thi.ng/transducers-hdom": "2.0.49",
    "d3-force": "2.0.1",
    "d3-require": "1.2.2",
    "requirejs": "2.3.6",
    "tslib": "1.10.0",
    "ws": "7.4.6"
  },
  "devDependencies": {
    "@types/ws": "6.0.3",
    "prettier": "2.0.2",
    "rollup": "1.26.0",
    "typescript": "4.0.0-beta"
  },
  "prettier": {
    "arrowParens": "avoid",
    "proseWrap": "always"
  }
}
